{
    "name": "alibabacloud/polardbx",
    "homepage": "https://www.alibabacloud.com/",
    "description": "Alibaba Cloud Polardbx SDK for PHP",
    "keywords": [
        "polardbx",
        "sdk",
        "cloud",
        "aliyun",
        "alibaba",
        "library",
        "alibabacloud"
    ],
    "type": "library",
    "license": "Apache-2.0",
    "support": {
        "source": "https://github.com/alibabacloud-sdk-php/polardbx",
        "issues": "https://github.com/alibabacloud-sdk-php/polardbx/issues"
    },
    "authors": [
        {
            "name": "Alibaba Cloud SDK",
            "email": "sdk-team@alibabacloud.com",
            "homepage": "http://www.alibabacloud.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "alibabacloud/client": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "AlibabaCloud\\Polardbx\\": ""
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
