{
  "name": "wgx954418992/rapid-php",
  "version": "3.9.7",
  "description": "Fast, simple and practical php framework",
  "keywords": [
    "framework",
    "rapidphp",
    "restful",
    "fastapi",
    "api"
  ],
  "homepage": "https://www.rapid-framework.com/",
  "license": "MIT",
  "authors": [
    {
      "name": "Renew",
      "email": "wgx954418992@qq.com"
    }
  ],
  "require": {
    "wgx954418992/rapid-framework": "^3.9.7",
    "ext-zip": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "eaglewu/swoole-ide-helper": "dev-master",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "files": [],
    "psr-4": {
      "apps\\": "apps/",
      "script\\": "scripts/"
    }
  },
  "scripts": {
    "generator-model-php": [
      "php scripts/model/index.php php --conf='${PATH_ROOT}apps/core/application.yaml ${PATH_ROOT}.script.yaml'"
    ],
    "generator-model-java": [
      "php scripts/model/index.php java --conf='${PATH_ROOT}apps/core/application.yaml ${PATH_ROOT}.script.yaml'"
    ],
    "generator-model-swift": [
      "php scripts/model/index.php swift --conf='${PATH_ROOT}apps/core/application.yaml ${PATH_ROOT}.script.yaml'"
    ],
    "php-conver-wrapper-to-swift": [
      "php scripts/convert/index.php wrapper/swift"
    ],
    "php-conver-bean-to-swift": [
      "php scripts/convert/index.php bean/swift"
    ],
    "docker-run": [
      "./scripts/docker/rapid-php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  }
}
