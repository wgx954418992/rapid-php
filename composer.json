{
  "name": "wgx954418992/rapid-php",
  "version": "3.4.0",
  "description": "Fast, simple and practical php framework",
  "keywords": [
    "framework",
    "rapidphp",
    "restful",
    "fastapi",
    "api"
  ],
  "homepage": "https://www.rapid-framework.com/",
  "license": "MIT",
  "authors": [
    {
      "name": "Renew",
      "email": "wgx954418992@qq.com"
    }
  ],
  "require": {
    "wgx954418992/rapid-framework": "^3.4.1"
  },
  "autoload": {
    "files": [],
    "psr-4": {
      "apps\\": "apps/",
      "script\\": "scripts/"
    }
  },
  "scripts": {
    "generator-model-php": [
      "php -r \"require __DIR__.'/vendor/autoload.php';script\\model\\classier\\ToModel::run([__DIR__.'/apps/app/application.yaml'],'sql','php',__DIR__.'/apps/app/classier/model/','apps\\app\\classier\\model');\""
    ],
    "generator-model-swift": [
      "php -r \"require __DIR__.'/vendor/autoload.php';script\\model\\classier\\ToModel::run([__DIR__.'/apps/app/application.yaml'],'sql','swift',__DIR__.'/models/swift/',null,['extends'=>'BaseModel','imports'=>['Foundation']]);\""
    ],
    "generator-model-java": [
      "php -r \"require __DIR__.'/vendor/autoload.php';script\\model\\classier\\ToModel::run([__DIR__.'/apps/app/application.yaml'],'sql','java',__DIR__.'/models/java/','models',['extends'=>'BaseModel']);\""
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  }
}
